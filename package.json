{
  "scripts": {
    "clean": "rimraf _site",
    "serve:eleventy": "eleventy --serve --incremental",
    "serve:postcss": "postcss src/assets/css/styles.css -o _site/css/styles.css --config ./postcss.config.js -w",
    "build:eleventy": "eleventy",
    "build:postcss": "postcss src/assets/css/styles.css -o _site/css/styles.css --config ./postcss.config.js",
    "dev": "cross-env NODE_ENV=development npm-run-all clean build:postcss -p serve:*",
    "build": "cross-env NODE_ENV=production  npm-run-all -s build:*"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.0-canary.18",
    "@11ty/eleventy-img": "^2.0.1",
    "@11tyrocks/eleventy-plugin-emoji-readtime": "^1.0.1",
    "@justinribeiro/lite-youtube": "^1.4.0",
    "@quasibit/eleventy-plugin-schema": "^1.10.0",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "alpinejs": "^3.10.5",
    "clipboard": "^2.0.11",
    "eleventy-plugin-metagen": "^1.7.11",
    "eleventy-plugin-svg-sprite": "^2.0.0",
    "m": "^1.8.2",
    "markdown-it-attrs": "^4.1.4",
    "markdown-it-link-attributes": "^4.0.1",
    "sharer.js": "^0.5.1",
    "tailwindcss": "^3.2.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "html-minifier": "^4.0.0",
    "netlify-plugin-cache": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "postcss-lightningcss": "^0.6.0",
    "rimraf": "^3.0.2"
  },
  "browserslist": [
    "defaults"
  ]
}
