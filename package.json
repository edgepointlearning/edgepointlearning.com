{
  "devDependencies": {
    "netlify-plugin-jekyll-cache": "^1.0.0",

    "foundation-sites": "^6.7.5",
    "motion-ui": "^2.0.4",
    "what-input": "^5.2.12",
    "jquery": "^3.6.0",

    "instant.page": "^5.1.0",
    "lazysizes": "^5.3.0",

    "netlify-cli": "^10.7.1"
  },
  "scripts": {
    "netlify": "netlify",
    "dev": "bundle exec jekyll serve --future --config ./src/_config.yml",
    "build": "bundle exec jekyll build --config ./src/_config.yml",
    "proof": "bundle exec htmlproofer ./_site --allow-hash-href --ignore-empty-mailto --directory-index-file index.html --checks-to-ignore ScriptCheck,ImageCheck",
    "proof-internal": "bundle exec htmlproofer ./_site --allow-hash-href --ignore-empty-mailto --directory-index-file index.html --disable_external --checks-to-ignore ScriptCheck,ImageCheck"
  }
}
