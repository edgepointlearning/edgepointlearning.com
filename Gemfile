group :jekyll_plugins do
  gem 'jekyll-assets'           # https://github.com/envygeeks/jekyll-assets
  gem 'jekyll-paginate-v2'      # https://github.com/sverrirs/jekyll-paginate-v2
  gem 'jekyll-redirect-from'    # https://github.com/jekyll/jekyll-redirect-from
  gem 'jekyll-sitemap'          # https://github.com/jekyll/jekyll-sitemap
  gem 'jekyll-algolia'          # https://github.com/algolia/jekyll-algolia
  gem 'jekyll-target-blank'     # https://github.com/keithmifsud/jekyll-target-blank
  #gem 'jekyll-inline-svg'  
end

gem 'sprockets', '~>3', { require: false }
gem 'nokogiri'
