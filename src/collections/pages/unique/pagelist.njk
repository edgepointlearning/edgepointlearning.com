---
title: pagelist
permalink: /list/
layout: layouts/01.nav.njk
sitemap:
  ignore: true
---
<div class="bg-white py-8">    
  <table class="wrapper mx-auto table-auto text-xs">
    {%- for post in collections.all -%}
      <tr>
        <td class="py-1">
          <a class="text-epl-light" href="{{ 'https://eleventy--edgepointlearning.netlify.app' ~ post.url }}">{{ post.url }}</a>
        </td>
        <td>
          {{ post.data.title }}
        </td>
        <td>
          {{ 'hidden' if post.data.sitemap.ignore }}
        </td>
      </tr>
    {%- endfor -%}
  </table>
</div>